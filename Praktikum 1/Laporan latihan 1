print("Hitung Luas Ruangan")
x=float(input("Masukan Panjang Ruangan: "))
y=float(input("Masukan Lebar Ruangan: "))
z=input("Masukan satuan (meter/inci): ")
if z=='meter':
    luas=x*y
    print("Luas Ruangan dengan panjang",x,z,"dan Lebar ruangan",y,z,"adalah",luas,"m²")
    print("terima kasih")
if z=='inci':
    luas=x*y
    print("Luas Ruangan dengan panjang",x,z,"dan Lebar ruangan",y,z,"adalah",luas,"in²")
    print("terima kasih")
